{% extends 'admin/_dashboard.html.twig' %}
{% block context %}
   <h1 class="f-upper fw fs-2">Ajouter une catégorie</h1>
    {{ form_start(categoryForm, { 'attr' : {'class' : 'form-admin' }}) }}
        {{ form_widget(categoryForm.label, { 'attr' : {'class' : 'input', 'placeholder' : 'Nom de Catégorie' }}) }}
        {{ form_widget(categoryForm.submit, { 'attr' : {'class' : 'btn bg-green' }} )}}
    {{ form_end(categoryForm) }}
{% endblock %}